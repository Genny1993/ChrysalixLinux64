#Ввод числа пользователем


var: $x;
jump: &first_input;
label: &repeat_input;
	print: 'Вы ввели не число, введите число еще раз: ';
jump: &input;
label: &first_input;
	print: 'Введите число: ';
label: &input;
	input: $x;
jifnot: (castable: 'ntg', $, $x), &repeat_input;
