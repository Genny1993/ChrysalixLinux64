#Замер времени выполнения
var: $time; hrt: $time;

var: $i, 0; label: &loop;
	calc: 'ceil', $, -6.9;
jif: (calc: 'inc', $i; comp: '<', $, $i, 1000000), &loop; free: $i;

print: (calc: '/', $, (to: 'dbl', $, (calc: '-', $, (hrt: $), $time)), 1000000000.0), '\n';

end: 0; 